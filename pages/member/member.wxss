/* pages/member/member.wxss */
.member{
  text-align: center
}
.avatar{
  margin: 100rpx auto 50rpx;
  width: 180rpx;
  height: 180rpx;
}
.name{
  display: block;
  color: #666

}
.time{
  margin: 30rpx 0 100rpx 0;
  display: block;
  color: #999;
  font-size: 12px;
}
.item{
  margin: 30rpx 10rpx 100rpx;
  font-size: 12px;
  line-height: 12px;
  display: inline-block;
  padding: 4px 4px 4px 4px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  border-radius: 2px;
  text-decoration: none;
  color: #999;
}